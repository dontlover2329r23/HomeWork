#include<iostream>
#include<Windows.h>
#include <stdio.h>
#include <stdlib.h>
using namespace std;


int main() {

	SetConsoleOutputCP(1251);
	SetConsoleCP(1251);
	int n  , a , b;
	char line[121];
	char g[3];
	char k[120];
	gets_s(line, 120);
	cout << "Введите количество символов";
	cin >> n;
	a= 0;
	b = 4;
	int c = 0;
	
	for (int i = n-1; i >=0; i--) {
		if (a == 0) {
			if (line[i] == ' ') {
				g[0] = line[i + 1];
				g[1] = '.';
				a++;
			}
		}
		else if ((a == 1)&&(line[i]==' ')) {
			k[0] = line[i+1];
			k[1] = '.';
			a++;
		} 
		else if ((a == 2) && (line[i] != ' ')) {
			k[b] = line[c];
			c++;
			b++;
		}
		
	}
	k[2] = g[0];
	k[3] = g[1];
	for (int i = b; i < 120; i++) {
		k[i] = '0';
	}
	for (int i = 0; i < 120; i++) {
		if (k[i] != '0') {
			cout << k[i];
		}
	}
	return 0;
}
