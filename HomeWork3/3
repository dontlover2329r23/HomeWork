#include <iostream>
using namespace std;
int Del(int x, int  a) {
	if (x > 1) {
		if (x % a == 0) {
			cout << a << " * ";
			if (a == 1)
				Del(x, a + 1); else {
				Del(x / a, a);
				return a, x;
			}
		}
		else {
			Del(x, a + 1);
			return a, x;
		}
	}
	if (x == 1) return a, x;
}
int main() {
	int x, a;
	a = 1;
	cin >> x;
	if (x == 1)
		cout << "x=1";
	if (x > 1) {
		cout << "x= ";
		Del(x, a);
	}
	return 0;
}
