#include <iostream>
#include <string>
using namespace std;
int main()
{
    setlocale(LC_ALL, "Rus");
    char str1;
    cin >> str1;
    char str2;
    cin >> str2;
    int N;
    cin >> N;
    char* str = new char[N];
    int i;
    for ( i = 0; i < N; i++) {
        str[i] = str1;
        str[i + 1] = str2;
        i += 1;
    }
    str[i ] = '\0';
    cout << str;
    return 0;
}
