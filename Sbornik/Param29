#include<iostream>
using namespace std;
void print(int** mas, int N, int M) {
    for (int i = 0; i < N; i++) {
        for (int j = 0; j < M; j++) {
            cout << mas[i][j] << " ";
        }
        cout << endl;
    }
}
    void Param29(int** mas, int N, int M) {
    for (int i = 0; i < M; i++) {
        for (int h = 1; h <= M - i - 1; h++) {
            bool bol = false;
            int k = 0;
            while (k < N) {
                if (mas[k][i] > mas[k][i + h]) {
                    bol = true;
                    break;
                }
                else if (mas[k][i] > mas[k][i+h]) break;
                    k++;
            }
            if (bol == true) {
            for (int j = 0; j < N ; j++)
            {
                    swap(mas[j][i], mas[j][i+h]);
            }

            }
        }
    }
    for (int i = 0; i < M; i++) {
        for (int h = 1; h <= M - i - 1; h++) {
            bool bol = false;
            int k = 0;
            while (k < N) {
                if (mas[k][i] > mas[k][i + h]) {
                    bol = true;
                    break;
                }
                else if (mas[k][i] > mas[k][i + h]) break;
                k++;
            }
            if (bol == true) {
                for (int j = 0; j < N; j++)
                {
                    swap(mas[j][i], mas[j][i + h]);
                }

            }
        }
    }
}
int main()
{
    int N, M;
    cin >> M>>N;
    int** C = new int* [M];
    for (int i = 0; i < M; i++) {
        C[i] = new int[N];
    }
    for (int i = 0; i < M; i++) {
        for (int j = 0; j < N; j++) {
            cin >> C[i][j];
        }
    }
    Param29(C, M, N);
    print(C, M,N);
    return 0;
}
