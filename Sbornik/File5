#include <iostream>
#include<Windows.h>
#include <stdio.h>
#include <stdlib.h>
#include<string>
#include<fstream>
using namespace std;
bool file5(string s1, int &k) {
    s1 += ".txt";
    ifstream fin(s1);
    if (!fin.is_open())
    {
        cout << "eror";
        return false;
    }
    int a;
    while (!fin.eof()) {
        fin >> a;
        k += 1;
    }

}
int main()
{
    system("chcp 1251");
    string s1;
    int k = 0;
    getline(cin, s1);
    if (file5(s1, k) == false) {
        cout << "-1";
    }
    else cout << k;
    return 0;
}
