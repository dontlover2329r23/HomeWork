#include <iostream>
#include <cctype>
using namespace std;

int main()
{
    setlocale(LC_ALL, "Rus");
    string str1 , str2, str3;
    cin >> str1;
    for (int i = 0; i < str1.size(); i++) {
        str2 += (char)tolower(str1[i]);
    }
    for (int i = 0; i < str2.size(); i++) {
        str3 += (char)toupper(str2[i]);
    }
    cout << str2;
    cout << str3;
    return 0;
}
