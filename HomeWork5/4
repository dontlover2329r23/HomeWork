#include<iostream>
#include<ctime>
using namespace std;
void random(int** mas, int n, int m) {
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < m; j++) {
			mas[i][j] = rand() % 9 + 1;
		}
	}
}
void print_arr(int** mas, int n, int m) {
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            cout << mas[i][j] << " ";
        }
        cout << endl;
    }
}
void swap1(int** mas, int n, int m) {
    int  max_int = INT_MAX;
    int i1=0;
    int j1 = 0;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            if ((mas[i][j] < max_int) && (mas[i][j] > -1)) {
                max_int = mas[i][j];
                i1 = i;
                j1 = j;
            }
        }
    }
    int g = 0;
    for (int i = 0; i < n; i++) {
        g = mas[i][j1];
        mas[i][j1] = mas[i1][i];
        mas[i1][i] = g;
    }
}
int main() {
        int n;
        cin >> n;
        int** mas = new int* [n];
        for (int i = 0; i < n; i++)
            mas[i] = new int[n];
        random(mas, n, n);
        print_arr(mas, n, n);
        swap1(mas, n, n);
        cout << endl;
        print_arr(mas, n, n);
        return 0;
}
