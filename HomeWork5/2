#include<iostream>
#include<ctime>
using namespace std;
void random(int** mas, int n, int m) {
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < m; j++) {
			mas[i][j] = rand() % 9 + 1;
		}
	}
}
void del_full_matr(int** mas, int n) {
    for (int i = 0; i != n; i++) {
        delete[] mas[i];
    }
}
void print_arr(int** mas, int n, int m) {
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            cout << mas[i][j] << " ";
        }
        cout << endl;
    }
}
void multiplication(int** c , int  n1 , int m2 , int m1 , int** a, int**b){
    for (int i = 0; i < n1; i++)
    {
        for (int j = 0; j < m2; j++)
        {
            c[i][j] = 0;
            for (int k = 0; k < m1; k++)
                c[i][j] += a[i][k] * b[k][j];
        }
    }
}
int main() {
    int n1, m1, m2, n2;
    bool bol = true;
    cin >> n1 >> m1;
    int** a = new int* [n1];
    for (int i = 0; i < n1; i++)
        a[i] = new int[m1];
    random(a, n1, m1);
    print_arr(a, n1, m1);
    cout << endl;
    cin >> n2 >> m2;
    if (m1 != n2) {
        bol = false;
    }

    int** b = new int* [n2];
    for (int i = 0; i < n2; i++)
        b[i] = new int[m2];
    random(b, n2, m2);
    print_arr(b, n2, m2);
    cout << endl;
    if (bol != false) {
        int** c = new int* [n1];
        for (int i = 0; i < n1; i++)
            c[i] = new int[m2];
        multiplication(c, n1, m2, m1, a, b );
        print_arr(c, n1, m2);
    }
    else cout << "Eror";
    del_full_matr(a,n1);
    del_full_matr(b,n2);
    del_full_matr(c,n1);
    return 0;
}
